{
  "name": "@clevercloud/components",
  "version": "5.5.0",
  "description": "This is a Work In Progress to introduce Web Components in our Clever Cloud Web based UIs.",
  "license": "Apache-2.0",
  "keywords": [
    "components",
    "clever-cloud"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/CleverCloud/clever-components.git"
  },
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "components:build": "rollup -c rollup/rollup-npm.config.js",
    "components:build-cdn": "rollup -c rollup/rollup-cdn.config.js",
    "components:build-cdn:versions-list": "node tasks/update-versions-list.js",
    "components:publish-cdn": "node tasks/publish-cdn.js",
    "components:check-i18n": "node tasks/check-i18n.js",
    "components:check-lit": "lit-analyzer src",
    "components:docs": "wca analyze src/** --outFile .components-docs/custom-elements.json",
    "test": "wtr 'test/**/*.test.*' --node-resolve",
    "test:watch": "wtr 'test/**/*.test.*' --node-resolve --watch",
    "lint": "eslint --rulesdir eslint-rules rollup/*.js src stories tasks",
    "lint:fix": "eslint --rulesdir eslint-rules --fix rollup/*.js src stories tasks",
    "prepack": "npm run components:build",
    "prestorybook:build": "npm run components:docs",
    "prestorybook:dev": "npm run components:docs",
    "start": "static -H '{\"cache-control\": \"no-cache, no-store, must-revalidate\", \"Pragma\": \"no-cache\", \"Expires\": \"0\"}' -a 0.0.0.0 -p 8080 storybook-static",
    "storybook:build": "build-storybook -o storybook-static${STORYBOOK_PATH}",
    "storybook:dev": "wds -p 6006"
  },
  "dependencies": {
    "@clevercloud/client": "^7.3.2",
    "chart.js": "^2.9.4",
    "chartjs-plugin-datalabels": "^0.7.0",
    "clipboard-copy": "^4.0.1",
    "highlight.js": "^10.4.1",
    "leaflet": "^1.7.1",
    "leaflet.heat": "^0.2.0",
    "lit-element": "^2.4.0",
    "lit-html": "^1.3.0",
    "resize-observer-polyfill": "^1.5.1",
    "rxjs": "7.0.0-beta.12",
    "statuses": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/preset-react": "^7.12.10",
    "@bundled-es-modules/chai": "^4.2.2",
    "@open-wc/dev-server-hmr": "^0.1.0",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@web/dev-server": "^0.1.3",
    "@web/dev-server-rollup": "^0.3.1",
    "@web/dev-server-storybook": "^0.3.2",
    "@web/rollup-plugin-import-meta-assets": "^1.0.4",
    "@web/test-runner": "^0.11.7",
    "@web/test-runner-mocha": "^0.6.0",
    "aws-sdk": "^2.808.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-inline-svg": "^1.2.0",
    "babel-plugin-template-html-minifier": "^4.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "dependency-graph": "^0.9.0",
    "eslint": "^7.15.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-lit": "^1.2.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-standard": "^5.0.0",
    "github-markdown-css": "^4.0.0",
    "glob": "^7.1.6",
    "hanbi": "^0.4.1",
    "i18n-extract": "^0.6.7",
    "js-yaml": "^3.14.1",
    "lit-analyzer": "^1.1.10",
    "mime-types": "^2.1.27",
    "node-static": "^0.7.11",
    "postcss": "^8.2.1",
    "postcss-sorting": "^6.0.0",
    "remark-frontmatter": "^3.0.0",
    "remark-highlight.js": "^6.0.0",
    "remark-html": "^13.0.1",
    "remark-parse": "^9.0.0",
    "rollup": "^2.34.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-visualizer": "^4.2.0",
    "superagent": "^6.1.0",
    "svgo": "^1.3.2",
    "unified": "^9.2.0",
    "web-component-analyzer": "^1.1.6"
  },
  "volta": {
    "node": "14.16.0",
    "npm": "7.6.1"
  }
}
